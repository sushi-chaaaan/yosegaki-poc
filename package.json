{
  "name": "itsuki-birthday",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "postinstall": "husky install",
    "eslint": "eslint ./src/**/*.{js,ts,jsx,tsx} --ignore-path .gitignore",
    "eslint:fix": "eslint --fix ./src/**/*.{js,ts,jsx,tsx} --ignore-path .gitignore",
    "eslint:precommit": "eslint --max-warnings 0 --ignore-path .gitignore",
    "format": "prettier --write --ignore-path .gitignore .",
    "format:precommit": "prettier --check --ignore-path .gitignore",
    "stylelint": "stylelint \"./src/**/*.{css,scss}\"",
    "stylelint:fix": "stylelint --fix \"./src/**/*.{css,scss}\"",
    "stylelint:precommit": "stylelint --max-warnings 0 --ignore-path .gitignore",
    "markuplint": "markuplint \"./**/*.{jsx,tsx}\"",
    "markuplint:precommit": "markuplint --no-color --problem-only \"./**/*.{jsx,tsx}\"",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "ncu": "ncu -u --target minor",
    "ncu:latest": "ncu --target latest",
    "tsx": "dotenv -e .env.local -- tsx",
    "supa:link": "supabase link",
    "supa:typegen": "supabase gen types typescript --linked > ./src/types/supabase.ts",
    "db:generate": "dotenv -e .env.local -- drizzle-kit generate:pg --config drizzle.config.ts",
    "db:push": "dotenv -e .env.local -- drizzle-kit push:pg --config drizzle.config.ts"
  },
  "lint-staged": {
    "./**/*.{js,ts}": [
      "npm run eslint:precommit",
      "npm run format:precommit"
    ],
    "./**/*.{jsx,tsx}": [
      "npm run eslint:precommit",
      "npm run format:precommit",
      "npm run markuplint:precommit"
    ],
    "./**/*.{css,scss}": "npm run stylelint:precommit"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-brands-svg-icons": "^6.5.1",
    "@fortawesome/free-regular-svg-icons": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@hookform/resolvers": "^3.3.2",
    "@next/bundle-analyzer": "^14.0.3",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-toast": "^1.1.5",
    "@supabase/ssr": "^0.0.10",
    "@supabase/supabase-js": "^2.39.0",
    "@t3-oss/env-nextjs": "^0.7.1",
    "@unocss/reset": "^0.58.0",
    "@uppy/core": "^3.7.1",
    "@uppy/tus": "^3.4.0",
    "budoux": "^0.6.1",
    "class-variance-authority": "^0.7.0",
    "client-only": "^0.0.1",
    "clsx": "^2.0.0",
    "drizzle-orm": "^0.29.1",
    "drizzle-zod": "^0.5.1",
    "eslint-plugin-drizzle": "^0.2.2",
    "framer-motion": "^10.16.12",
    "next": "14.0.3",
    "nextjs-toploader": "^1.6.4",
    "postgres": "^3.4.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "react-loading-skeleton": "^3.3.1",
    "server-only": "^0.0.1",
    "sharp": "^0.33.0",
    "tailwind-merge": "^2.1.0",
    "tailwind-variants": "^0.1.18",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@markuplint/jsx-parser": "^3.12.0",
    "@markuplint/react-spec": "^3.13.0",
    "@tailwindcss/typography": "^0.5.10",
    "@types/eslint": "8.44.8",
    "@types/node": "20.10.3",
    "@types/react": "^18.2.41",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "autoprefixer": "^10.4.16",
    "daisyui": "^4.4.18",
    "dotenv-cli": "^7.3.0",
    "drizzle-kit": "^0.20.6",
    "eslint": "^8.55.0",
    "eslint-config-next": "14.0.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-perfectionist": "^2.5.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "markuplint": "^3.15.0",
    "npm-check-updates": "^16.14.11",
    "postcss": "^8",
    "prettier": "^3.1.0",
    "sass": "^1.69.5",
    "stylelint": "^15.11.0",
    "stylelint-a11y": "^1.2.3",
    "stylelint-config-recess-order": "^4.4.0",
    "stylelint-config-sass-guidelines": "^10.0.0",
    "stylelint-config-standard-scss": "^11.1.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.7.0",
    "stylelint-value-no-unknown-custom-properties": "^5.0.0",
    "supabase": "1.117.1",
    "tailwindcss": "^3.3.5",
    "tsx": "^4.6.2",
    "typescript": "^5.3.2"
  },
  "volta": {
    "node": "18.18.2",
    "yarn": "4.0.2"
  }
}
